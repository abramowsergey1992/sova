mixin paginator()
	.paginator
		a(href="#").paginator__prev
			<svg width="15" height="15" viewBox="0 0 15 15" fill="none" xmlns="http://www.w3.org/2000/svg">
				<path d="M9.5 2.5L4.5 7.5L9.5 12.5" stroke="#828D9E" stroke-linecap="square"/>
			</svg>

		a(href="#").paginator__page._active
			span 1
		.paginator__more 
			span ...
		a(href="#").paginator__page._hidden
			span 2
		a(href="#").paginator__page._hidden
			span 3
		a(href="#").paginator__page 
			span 4
		a(href="#").paginator__page 
			span 5
		a(href="#").paginator__page 
			span 6
		a(href="#").paginator__next
			<svg width="15" height="15" viewBox="0 0 15 15" fill="none" xmlns="http://www.w3.org/2000/svg">
				<path d="M5.5 2.5L10.5 7.5L5.5 12.5" stroke="#828D9E" stroke-linecap="square"/>
			</svg>

